; BIND reverse data file for empty rfc1918 zone
;
; DO NOT EDIT THIS FILE - it is used for multiple zones.
; Instead, copy it, edit named.conf, and use that copy.
;
$TTL	86400
@	IN	SOA	ns.prj.pt. admin.prj.pt. (
			      1	; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
;
@	IN	NS	ns.prj.pt.
@	IN	NS	ns02.prj.pt.
@	IN	MX	10 mx01.prj.pt.
ns	IN	A	10.10.20.10
ns02	IN	A	10.10.20.60
mx01	IN	A	10.10.20.20
home	IN	A	10.10.20.30
market	IN	A	10.10.20.30
web1	IN	A	10.10.20.50
